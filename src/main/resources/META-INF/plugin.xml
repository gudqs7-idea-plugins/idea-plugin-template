<idea-plugin>
    <id>gudqs7.github.io.xxx</id>
    <name>Xxx</name>
    <vendor email="gudqs7@gmail.com" url="https://github.com/gudqs7">gudqs7</vendor>

    <description><![CDATA[ Description will be added by gradle build]]></description>

    <idea-version since-build="232" />

    <!-- please see https://www.jetbrains.org/intellij/sdk/docs/basics/getting_started/plugin_compatibility.html
         on how to target different products -->
    <depends>com.intellij.modules.platform</depends>
    <depends>com.intellij.modules.lang</depends>
    <depends optional="true" config-file="all/rust.xml">com.jetbrains.rust</depends>
    <depends optional="true" config-file="all/java.xml">com.intellij.modules.java</depends>


    <extensions defaultExtensionNs="com.intellij">
        <notificationGroup displayType="BALLOON" id="Api Savior Tip" bundle="group" key="notification.group.tip.name"/>
        <notificationGroup displayType="STICKY_BALLOON" id="Api Savior Error" bundle="group" key="notification.group.error.name"/>

        <errorHandler implementation="cn.gudqs7.plugins.diagnostic.XxxIssueSubmitter"/>
    </extensions>

    <actions>

    </actions>


</idea-plugin>